{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema",
  "required": [
    "cluster",
    "website"
  ],
  "properties": {
    "cluster": {
      "type": "string",
      "description": "destination cluster",
      "default": "dummy"
    },
    "website": {
      "type": "object",
      "description": "Website parameters section",
      "required": [
        "title"
      ],
      "properties": {
        "title": {
          "type": "string",
          "description": "Title of the website",
          "default": "NGINX"
        }
      }
    }
  }
}